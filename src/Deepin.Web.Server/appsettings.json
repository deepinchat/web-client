      "download": "download",
      "upload": "upload",
      "chat": "chat",
      "message": "message",
      "presence": "presence",
      "user": "user"
    }
  },
  "DeepinApiUrl": "http://localhost:5000",
  "OpenApi": {
    "Endpoint": {
      "Name": "Deepin Web Server V1"
    },
    "Document": {
      "Description": "The Deepin Web Server HTTP API",
      "Title": "deepin - Web Server HTTP API",
